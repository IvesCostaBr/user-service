    "version": "0.2.0",
    "configurations": [
        {
            "name": "Auth Api",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main_api:app",
                "--port",
                "7000",
            ],
            "jinja": true,
            "justMyCode": true
        },
        {
            "name": "Debug Container Api",
            "type": "python",
            "request": "attach",
            "port": 5678,
            "host": "localhost",
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}",
                    "remoteRoot": "/code"
                }
            ]
        }
    ]
}